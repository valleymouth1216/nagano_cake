 <div class='container'>
<h2>管理者ログイン</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <label for="emain">メールアドレス　</label>
    <%= f.email_field :email,id:"email", autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <label for="password">パスワード　</label>
    <%= f.password_field :password,id:"password",  autocomplete: "current-password" %>
  </div>



  <div class="actions">
    <%= f.submit "ログイン",class:"btn btn-sm btn-success"%>
  </div>
<% end %>

<%= render "admin/shared/links" %>

 </div>